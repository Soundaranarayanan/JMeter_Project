        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Error Rate" enabled="false">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">// Get global stats from JMeter context
long total = SampleResult.getSampleCount()
long errors = SampleResult.getErrorCount()

if (total &gt; 0) {
    double errorRate = (errors * 100.0) / total
    if (errorRate &gt; 2.0) {
        log.error(&quot;⚠️ Error rate of pim exceeded 2%: ${errorRate}%&quot;)
        Failure = true  // mark assertion as failed
    } else {
        log.info(&quot;✅ Error rate is within acceptable range: ${errorRate}%&quot;)
    }
}</stringProp>
        </JSR223Sampler>
