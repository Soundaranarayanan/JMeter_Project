        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Pacing" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// Amount of pacing you want after each iteration (in ms)
long pacingOffset = 10000  // 10 seconds

// Get iteration start time; if not found, use current time
long startTime = vars.get(&quot;iterationStart&quot;)?.toLong() ?: System.currentTimeMillis()
long now = System.currentTimeMillis()

// Time spent in current iteration
long elapsed = now - startTime

long totalTime = elapsed + pacingOffset
long waitTime = totalTime - elapsed 

int currentIter = vars.getIteration()
int loopCount = Integer.parseInt(vars.get(&quot;loop_count&quot;))

if (currentIter &lt; loopCount) {
    if (waitTime &gt; 0) {
        log.info(&quot;Iteration took ${elapsed} ms. Applying pacing delay of ${waitTime} ms&quot;)
        Thread.sleep(waitTime)
    } else {
        log.info(&quot;No pacing needed. Iteration already took longer than target.&quot;)
    }
} else {
    log.info(&quot;Final iteration - skipping pacing&quot;)
}
vars.put(&quot;iterationStart&quot;, System.currentTimeMillis().toString())
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
