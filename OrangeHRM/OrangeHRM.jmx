        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">def inputFile = new File(&quot;C:/apache-jmeter-5.6.3/JmeterSecondIteration/CsvUtilities/Simple_DataWriter.csv&quot;)
def outputFile = new File(&quot;C:/apache-jmeter-5.6.3/JmeterSecondIteration/CsvUtilities/Simple_DataWriter.csv&quot;)

if (!inputFile.exists()) {
    log.error(&quot;❌ Input file not found at: ${inputFile.absolutePath}&quot;)
    return
}

def lines = inputFile.readLines(&quot;UTF-8&quot;)

if (lines.size() &lt; 2) {
    log.warn(&quot;⚠️ CSV has no data rows (only header or empty). Skipping processing.&quot;)
    return
}

def header = lines[0]
def updatedHeader = header + &quot;,elapsedInSeconds&quot;
def separator = header.contains(&quot;\t&quot;) ? &quot;\t&quot; : &quot;,&quot;

def updatedLines = [updatedHeader]

lines.drop(1).each { line -&gt;
    def cols = line.split(separator, -1)
    try {
        def elapsedMs = cols[1].toInteger()
        def elapsedSec = (elapsedMs / 1000.0).round(3)
        updatedLines &lt;&lt; line + &quot;${separator}${elapsedSec}&quot;
    } catch (Exception e) {
        log.warn(&quot;⚠️ Could not convert line: ${line}&quot;)
        updatedLines &lt;&lt; line + &quot;${separator}&quot;
    }
}

outputFile.text = updatedLines.join(&quot;\n&quot;)
log.info(&quot;✅ File converted successfully! Output: ${outputFile.absolutePath}&quot;)
</stringProp>
